{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "storynia",
  "version": "0.1.0",
  "identifier": "com.storynia.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "storynia",
        "width": 1366,
        "height": 768,
        "resizable": false
      }
    ],
    "security": {
      "csp": "default-src 'self' ipc: http://ipc.localhost; img-src 'self' asset: http://asset.localhost",
      "assetProtocol": {
        "enable": true,
        "scope": [
          "*/**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "cublas64_12.dll",
      "cublasLt64_12.dll",
      "cudart64_12.dll"
    ]
  }
}